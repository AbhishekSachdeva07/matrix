#include <stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    int arr[a][4];
    for (int i=0; i<a; ++i){
        for (int j=0; j<4; ++j){
            int b;
            scanf("%d",&b);
            arr[i][j]=b;
        }
    }
    for (int i=0; i<a; ++i){
        for (int j=0; j<4; ++j){
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    } 
    
    return 0;
}
